  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "{{colors.on_surface_variant.default.hex}}",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "{{colors.tertiary.default.hex}}"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "{{colors.secondary.default.hex}}"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "{{colors.primary.default.hex}}",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "{{colors.on_surface.default.hex}}"
      }
    },
    {
      "scope": ["variable.other.property", "support.type.property-name"],
      "settings": {
        "foreground": "{{colors.tertiary.default.hex}}"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "{{colors.primary.default.hex}}"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "{{colors.secondary.default.hex}}"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "{{colors.tertiary.default.hex}}"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "{{colors.primary.default.hex}}"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "{{colors.on_surface_variant.default.hex}}"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "{{colors.on_surface.default.hex}}"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "{{colors.on_surface_variant.default.hex}}"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "{{colors.primary.default.hex}}",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "{{colors.primary.default.hex}}",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "{{colors.tertiary.default.hex}}"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "{{colors.error.default.hex}}"
      }
    }
  ]
}